@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix sdir: <https://www.sdir.no/SDIR_Simulator#> .

sdir:FOR1404P23 a sdir:Requirement, sh:NodeShape;
  sdir:regulationReference "https://lovdata.no/forskrift/2013-11-22-1404/§23"^^xsd:anyURI;
  sh:property sdir:PS_MP_min50, sdir:PS_MP_max50 .

sdir:PS_MP_min50 a sh:NodeShape, sdir:Scope;
  sh:description "Scope of machine power more than 50V."@en, "Virkeområde maskinkraft mer enn 50V."@no;
  sh:minInclusive "50"^^xsd:int;
  sh:datatype unit:V;
  sh:minCount "1"^^xsd:int;
  sh:maxCount "1"^^xsd:int .

sdir:PS_MP_max50 a sh:NodeShape, sdir:Scope;
  sh:description "Scope of machine power up to 50V."@en, "Virkeområde maskinkraft opp til 50V."@no;
  sh:maxExclusive "50"^^xsd:int;
  sh:datatype unit:V;
  sh:minCount "1"^^xsd:int;
  sh:maxCount "1"^^xsd:int .
